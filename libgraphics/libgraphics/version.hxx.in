#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
// 
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
// 
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
// 
// Version      AAAAABBBBBCCCCCDDDE
// 
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
// 
#define LIBGRAPHICS_VERSION       $libgraphics.version.project_number$ULL
#define LIBGRAPHICS_VERSION_STR   "$libgraphics.version.project$"
#define LIBGRAPHICS_VERSION_ID    "$libgraphics.version.project_id$"
#define LIBGRAPHICS_VERSION_FULL  "$libgraphics.version$"

#define LIBGRAPHICS_VERSION_MAJOR $libgraphics.version.major$
#define LIBGRAPHICS_VERSION_MINOR $libgraphics.version.minor$
#define LIBGRAPHICS_VERSION_PATCH $libgraphics.version.patch$

#define LIBGRAPHICS_PRE_RELEASE   $libgraphics.version.pre_release$

#define LIBGRAPHICS_SNAPSHOT_SN   $libgraphics.version.snapshot_sn$ULL
#define LIBGRAPHICS_SNAPSHOT_ID   "$libgraphics.version.snapshot_id$"
