
# component Size : Number

# struct Size
# 	Number x
# 	Number y

component Position
	Number pos_x
	Number pos_y
	Number pos_z

component Velocity
	Number vel_x
	Number vel_y
	Number vel_z

system	onTickPhysics( anyEntity<	Position, Velocity> ) event Tick( Number deltaTime )
	anyEntity.Position.pos_z = anyEntity.Position.pos_x + (2 * 2 / 234324 * anyEntity.Velocity.vel_x)# * deltaTime
	anyEntity.Position.pos_y = anyEntity.Position.pos_y + anyEntity.Velocity.vel_y# * deltaTime
	anyEntity.Position.pos_z = anyEntity.Position.pos_z + anyEntity.Velocity.vel_z# * deltaTime
