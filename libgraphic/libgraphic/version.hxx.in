#pragma once

// The numeric version format is AAAAABBBBBCCCCCDDDE where:
// 
// AAAAA - major version number
// BBBBB - minor version number
// CCCCC - bugfix version number
// DDD   - alpha / beta (DDD + 500) version number
// E     - final (0) / snapshot (1)
// 
// When DDDE is not 0, 1 is subtracted from AAAAABBBBBCCCCC. For example:
// 
// Version      AAAAABBBBBCCCCCDDDE
// 
// 0.1.0        0000000001000000000
// 0.1.2        0000000001000020000
// 1.2.3        0000100002000030000
// 2.2.0-a.1    0000200001999990010
// 3.0.0-b.2    0000299999999995020
// 2.2.0-a.1.z  0000200001999990011
// 
#define LIBGRAPHIC_VERSION       $libgraphic.version.project_number$ULL
#define LIBGRAPHIC_VERSION_STR   "$libgraphic.version.project$"
#define LIBGRAPHIC_VERSION_ID    "$libgraphic.version.project_id$"
#define LIBGRAPHIC_VERSION_FULL  "$libgraphic.version$"

#define LIBGRAPHIC_VERSION_MAJOR $libgraphic.version.major$
#define LIBGRAPHIC_VERSION_MINOR $libgraphic.version.minor$
#define LIBGRAPHIC_VERSION_PATCH $libgraphic.version.patch$

#define LIBGRAPHIC_PRE_RELEASE   $libgraphic.version.pre_release$

#define LIBGRAPHIC_SNAPSHOT_SN   $libgraphic.version.snapshot_sn$ULL
#define LIBGRAPHIC_SNAPSHOT_ID   "$libgraphic.version.snapshot_id$"
